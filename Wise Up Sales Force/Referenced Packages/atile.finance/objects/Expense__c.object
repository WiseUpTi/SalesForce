{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)} 

var registros = {!GETRECORDIDS($ObjectType.Expense__c)}; 
  
if (registros[0] == null) { 
   alert(&quot;Select an expense&quot;) 
} else{
   window.location.href = &apos;/apex/afinance__new_transaction?ids=&apos; + registros;
}</url>
