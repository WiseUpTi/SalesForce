      }
      var nova = url.replace(tirar, param);  
      window.location.href = nova;
   }else{
      alert(&apos;E-mail do cliente não localizado!&apos;);
   }
}else{
   alert(&apos;Não é permitido reenviar e-mail nesta fase!&apos;);
