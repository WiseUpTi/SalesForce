    <validationRules>
        <fullName>Change_Status_without_Installment</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 NOT( ISPICKVAL( Status__c, &apos;Draft&apos; ) ),
 Amount__c = 0
)</errorConditionFormula>
        <errorMessage>To change from Draft to other status, you need to have a installment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_One_Relationship</fullName>
        <active>true</active>
        <errorConditionFormula>(
 IF( ISBLANK( Campaign__c ), 0, 1 ) +
 IF( ISBLANK( Account__c ), 0, 1 ) +
 IF( ISBLANK( Lead__c ), 0, 1 ) +
 IF( ISBLANK( Opportunity__c ), 0, 1 ) +
 IF( ISBLANK( Case__c  ), 0, 1 )
) &gt; 1</errorConditionFormula>
        <errorMessage>Select only one relationship in section Related to</errorMessage>
    </validationRules>
