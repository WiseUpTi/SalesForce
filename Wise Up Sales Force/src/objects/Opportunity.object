      }
      var nova = url.replace(tirar, param);  
      window.location.href = nova;
   }else if(idContaPF != &apos;&apos; &amp;&amp; idContaPF != null){
      var url = window.location.href; 
      var tudo = url.split(&quot;/&quot;); 
      var tirar = tudo[tudo.length - 1]; 

      if(bandeira == &apos;You Move&apos;){
         param = &apos;_ui/core/email/author/EmailAuthor?rtype=003&amp;p2_lkid=&apos; + idContaPF + &apos;&amp;p3_lkid={!Opportunity.Id}&amp;retURL=%2F{!Opportunity.Id}&amp;p5={!$User.Email}&amp;template_id=00X36000001CE1L&apos;; 
      }
      else{
         
        param = &apos;_ui/core/email/author/EmailAuthor?rtype=003&amp;p2_lkid=&apos; + idContaPF + &apos;&amp;p3_lkid={!Opportunity.Id}&amp;retURL=%2F{!Opportunity.Id}&amp;p5={!$User.Email}&amp;template_id=00X36000001CE1G&apos;;
      }
      
      var nova = url.replace(tirar, param);
      window.location.href = nova;
   }else if(emailConta != &apos;&apos; &amp;&amp; emailConta != null){
      var url = window.location.href; 
      var tudo = url.split(&quot;/&quot;); 
      var tirar = tudo[tudo.length - 1]; 
      if(bandeira == &apos;You Move&apos;){
         
         param = &apos;_ui/core/email/author/EmailAuthor?rtype=003&amp;p3_lkid={!Opportunity.Id}&amp;retURL=%2F{!Opportunity.Id}&amp;p5={!$User.Email}&amp;p24=&apos; + emailConta + &apos;&amp;template_id=00X36000001CE1L&apos;;
