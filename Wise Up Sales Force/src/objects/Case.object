    <validationRules>
        <fullName>afinance__Require_Number_of_payments</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   afinance__Generate_Transactions__c,
   OR(   
      ISBLANK(  afinance__Payment_Option__r.afinance__Number_Payments__c  ),
      afinance__Payment_Option__r.afinance__Number_Payments__c = 0,
      ISPICKVAL( afinance__Payment_Option__r.afinance__Payment_Schedule__c, &apos;&apos;),
      ISBLANK( afinance__Amount__c ),
      afinance__Amount__c = 0
   )
)</errorConditionFormula>
        <errorMessage>To generate transactions inform the number of payments, amount and payment schedule</errorMessage>
    </validationRules>
