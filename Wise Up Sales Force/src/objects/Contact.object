        <errorConditionFormula>AND(OR( ISPICKVAL(Pais_endereco__c , &apos;Brasil&apos;), ISBLANK(TEXT(Pais_endereco__c))), 
NOT( 
OR( 
LEN ( CEP_endereco__c ) = 0, 
REGEX( CEP_endereco__c, &quot;[0-9]{5}-[0-9]{3}&quot;) 
) 
) 
)</errorConditionFormula>
        <errorDisplayField>CEP_endereco__c</errorDisplayField>
        <errorMessage>Digite o cep no formato 99999-999</errorMessage>
    </validationRules>
