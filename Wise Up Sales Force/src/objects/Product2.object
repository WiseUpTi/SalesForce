    <validationRules>
        <fullName>Contrato_tipo_diferente_matricula</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISPICKVAL( Tipo__c ,&apos;Matrícula&apos;)), NOT(ISBLANK( Contrato_padrao__c )))</errorConditionFormula>
        <errorDisplayField>Contrato_padrao__c</errorDisplayField>
        <errorMessage>O contrato somente pode ser preenchido em produtos do tipo matrícula</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Duracao_obrigatoria_cobranca_recorrente</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( Forma_cobranca__c , &apos;Recorrente&apos;),
OR(ISBLANK( Duracao_contrato__c ),
Duracao_contrato__c &lt; 1)
)</errorConditionFormula>
        <errorDisplayField>Duracao_contrato__c</errorDisplayField>
        <errorMessage>Quando a forma de cobrança é recorrente, você deve informar um valor no campo Duração do contrato.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Modelo_de_contrato_requerido_matricula</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Tipo__c ,&apos;Matrícula&apos;), ISBLANK( Contrato_padrao__c ))</errorConditionFormula>
        <errorDisplayField>Contrato_padrao__c</errorDisplayField>
        <errorMessage>O contrato padrão é requerido para produtos do tipo matrícula</errorMessage>
    </validationRules>
