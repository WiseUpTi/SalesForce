            <script>
                $('.espelho_um').on('keyup', function() {
                    $('.'+$("p.numero-card").attr('class')).text($(this).val());
                    try{
                        var number = 0;
                        if($(this).val().length <= 3){
                            number = parseInt($(this).val());
                        } else{
                            number = parseInt($(this).val().substring(0,3));
                        }
                        try{
                            
                            if(number == 4 
                                    || (number >=40 && number <= 49) 
                                    || (number >= 400 && number <= 499)){
                                var visaFlag = $('img[data-val=Visa]');
                                $('.btnBandeiraClick').removeClass('selecionado').addClass('nao-selecionado');
                                visaFlag.removeClass('nao-selecionado').addClass('selecionado');
                                $('.mainCardFlag').val('Visa');
                            } else if((number  >= 50 && number <= 59)
                                            || (number >= 500 && number <= 599) ){                                
                                var masterFlag = $('img[data-val=Mastercard]');
                                $('.btnBandeiraClick').removeClass('selecionado').addClass('nao-selecionado');
                                masterFlag.removeClass('nao-selecionado').addClass('selecionado');
                                $('.mainCardFlag').val('Mastercard');
                            } else if((number >= 38 && number <= 39)
                                        || number == 36
                                        || number == 309
                                        || (number >= 300 && number <= 305 )
                                        || (number >= 360 && number <= 360)
                                        || (number >= 380 && number <= 399)){
                                                
                                var masterFlag = $('img[data-val=Diners]');
                                $('.btnBandeiraClick').removeClass('selecionado').addClass('nao-selecionado');
                                masterFlag.removeClass('nao-selecionado').addClass('selecionado');
                                $('.mainCardFlag').val('Diners');
                            } else if( (number == 34 || number == 37)
                                        || (number >= 340 && number <= 349 )
                                        || (number >= 370 && number <= 379))
                                    {
                                                
                                var masterFlag = $('img[data-val=Amex]');
                                $('.btnBandeiraClick').removeClass('selecionado').addClass('nao-selecionado');
                                masterFlag.removeClass('nao-selecionado').addClass('selecionado');
                                $('.mainCardFlag').val('Amex');
                            }                          
                            
                        }catch(e){
                        }
                    }catch(e){
                    }                    
                });
                    
                $('.espelho_dois').on('keyup', function() {
                    $('.'+$("p.validade-card").attr('class')).text($(this).val());
                });
                    
                $('.espelho_tres').on('keyup', function() {
                    $('.'+$("p.codigo-card").attr('class')).text($(this).val());
                });
                    
                $('.espelho_quatro').on('keyup', function() {
                    $('.'+$("p.nome-card").attr('class')).text($(this).val());
                    $("p.nome-card").css("text-transform","uppercase");
                });
            </script>
