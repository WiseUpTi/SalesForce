	OR( 
	LEN ( OtherPhone ) = 0, 
	REGEX( OtherPhone , &quot;[+]55 [0-9]{2} [0-9]{4,5} [0-9]{4}&quot;), 	
	REGEX( OtherPhone , &quot;0800 [0-9]{7}&quot;)
	) 
), false)</errorConditionFormula>
